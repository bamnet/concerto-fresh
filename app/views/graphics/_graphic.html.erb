<div id="<%= dom_id graphic %>">
  <p>
    <strong>Name:</strong>
    <%= graphic.name %>
  </p>

  <p>
    <strong>Duration:</strong>
    <%= graphic.duration %>
  </p>

  <p>
    <strong>Start time:</strong>
    <%= graphic.start_time %>
  </p>

  <p>
    <strong>End time:</strong>
    <%= graphic.end_time %>
  </p>

  <p>
    <strong>Image:</strong>
    <%= link_to graphic.image.filename, graphic.image if graphic.image.attached? %>
  </p>

</div>
